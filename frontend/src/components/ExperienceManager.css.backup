.remove-btn {
  background: #ef4444;
  color: white;
  border: none;
  width: 32px;
  height: 32px;
  border-radius: 50%;
  cursor: pointer;
  display: flex;
  align-items: center;
  justify-content: center;
  font-size: 1.2rem;
  font-weight: bold;
  transition: background 0.3s ease;
  flex-shrink: 0;
}

.remove-btn:hover {
  background: #dc2626;
}

.add-array-btn {
  background: none;
  border: 2px dashed #d1d5db;
  color: #6b7280;
  padding: 0.75rem;
  border-radius: 8px;
  cursor: pointer;
  font-size: 0.9rem;
  transition: all 0.3s ease;
  width: 100%;
}

.add-array-btn:hover {
  border-color: #667eea;
  color: #667eea;
  background: rgba(102, 126, 234, 0.05);
}

/* Form Actions */
.form-actions {
  display: flex;
  gap: 1rem;
  justify-content: flex-end;
  margin-top: 1rem;
  padding-top: 1rem;
  border-top: 1px solid #e5e7eb;
}

.cancel-btn,
.save-btn {
  padding: 0.75rem 1.5rem;
  border-radius: 8px;
  font-size: 1rem;
  font-weight: 500;
  cursor: pointer;
  transition: all 0.3s ease;
  border: none;
}

.cancel-btn {
  background: #f3f4f6;
  color: #374151;
}

.cancel-btn:hover {
  background: #e5e7eb;
}

.save-btn {
  background: linear-gradient(135deg, #667eea 0%, #764ba2 100%);
  color: white;
}

.save-btn:hover {
  transform: translateY(-1px);
  box-shadow: 0 4px 12px rgba(102, 126, 234, 0.3);
}

/* Responsive Design */
@media (max-width: 768px) {
  .experience-manager {
    padding: 1rem;
  }

  .experience-header {
    flex-direction: column;
    align-items: stretch;
    gap: 1rem;
  }

  .experience-header h2 {
    text-align: center;
    font-size: 1.5rem;
  }

  .add-experience-btn {
    justify-content: center;
  }

  .experience-header-card {
    flex-direction: column;
    gap: 0.5rem;
  }

  .experience-actions {
    align-self: flex-end;
  }

  .form-row {
    grid-template-columns: 1fr;
    gap: 1rem;
  }

  .form-actions {
    flex-direction: column-reverse;
  }

  .cancel-btn,
  .save-btn {
    width: 100%;
  }

  .modal-content {
    margin: 1rem;
    max-height: calc(100vh - 2rem);
  }

  .experience-modal {
    padding: 1.5rem;
  }
}

@media (max-width: 480px) {
  .experience-card {
    padding: 1rem;
  }

  .experience-info h3 {
    font-size: 1.1rem;
  }

  .experience-info h4 {
    font-size: 1rem;
  }

  .location-date {
    font-size: 0.85rem;
  }

  .tech-tags {
    gap: 0.3rem;
  }

  .tech-tag {
    font-size: 0.8rem;
    padding: 0.25rem 0.6rem;
  }
}
